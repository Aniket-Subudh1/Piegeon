<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".getStarted"
    android:background="@color/cream">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/_148sdp"
        android:layout_height="@dimen/_214sdp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="122dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/d" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/_535sdp"
        android:layout_height="@dimen/_510sdp"
        android:layout_marginTop="@dimen/_340sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.364"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.435"
        app:srcCompat="@drawable/bottom_white" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="@dimen/_193sdp"
        android:layout_height="@dimen/_73sdp"
        android:fontFamily="@font/spartan_bold"
        android:text="Get Started with PIGEON"
        android:textColor="@color/black"
        android:textSize="@dimen/_25sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.705" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="@dimen/_280sdp"
        android:layout_height="@dimen/_50sdp"
        android:fontFamily="@font/spartan_thin"
        android:text="&quot; We deliver messages with Real Encryption &quot;"
        android:textColor="@color/black"
        android:textSize="@dimen/_16sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.862" />

    <TextView
        android:id="@+id/login_text_view"
        android:layout_width="@dimen/_175sdp"
        android:layout_height="@dimen/_65sdp"
        android:background="@drawable/background_button"
        android:gravity="center"
        android:text="Get started"
        android:textColor="@color/black"
        android:textSize="@dimen/_18sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        android:fontFamily="@font/spartan_bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>